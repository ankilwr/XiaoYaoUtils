<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="AppThemeBar">
        <attr name="barTitle" format="string"/>
        <attr name="barTitleColor" format="color" />
        <attr name="leftIcon" format="reference" />
        <attr name="leftIconTint" format="color" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightIconTint" format="color" />
        <attr name="rightMenu" format="string" />
        <attr name="rightMenuTint" format="color" />
        <attr name="actionbarLineEnable" format="boolean" />
        <attr name="actionbarLineColor" format="color" />
        <attr name="fullStatusBar" format="boolean" />
        <attr name="lightStatusBarModel" format="boolean" />
    </declare-styleable>



    <declare-styleable name="HomeTabButton">
        <attr name="top_on" format="reference" />
        <attr name="top_off" format="reference" />
        <attr name="textColor_on" format="reference|color" />
        <attr name="textColor_off" format="reference|color" />
        <attr name="background_on" format="reference" />
        <attr name="background_off" format="reference" />
        <attr name="checked" format="boolean" />

        <attr name="image_width" format="dimension" />
        <attr name="image_height" format="dimension" />
        <attr name="content" format="reference" />
        <attr name="content_size" format="dimension" />

    </declare-styleable>


    <declare-styleable name="ProportionImageView">
        <attr name="ratio" format="float" />
    </declare-styleable>

</resources>
